//3

let number: Int? = 10

let num1: Int = number!

let num2 = number!

if let num3 = number {
    print("successful \(num3)")
}   else{
    print("nil")
}

var num4 = number ?? 15
print(num4)

func guardTest(){
    guard let num5 = number else{
        return
    }
    print(num5)
}
guardTest()

if case let num6? = number{
    print(num6)
}


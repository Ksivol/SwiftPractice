//5
let array1 = [Int]() 
let array2:[Int] = [] 
let array3: [String] = [] 

var array4 = [1, 2, 3] 
print(array4[0]) 
print(array4[2]) 
array4[1...2] = [1]
array4[0] = 4 
array4.append(4) 

var array5 = [1, 2, 3] 
array5[0] = array5[1] 
array5[0...1] = [4, 5] 
array5[0] = 6 
var i=0
while i<array5.count {
    array5[i] += 6
    i += 1
}
for item in array5 { print(item) } 

let dict1 = Dictionary<Int, Int>() 
let dict2 = [Int: Int]()
let dict3: [Int: Int] = [:]

var dict4 = ["One": 1, "Two": 2, "Three": 3] 
print(dict4["One"]) 
dict4["One"] = 1
dict4["Zero"] = 0 
dict4["Four"] = 4 

var dict5 = ["NY": "New York", "CA": "California"] 
dict5["NY"] = "New York"
dict5["WA"] = "Washington" 
dict5["CA"] = nil 

